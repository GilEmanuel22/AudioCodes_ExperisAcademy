                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT c.CUST, c.CUSTNAME AS Name, d.ADDRESS, d.CITY, s.STATENAME,co.COUNTRY

FROM mrr_CUSTOMERS C 

JOIN mrr_INVOICES i ON c.CUST = i.CUST

LEFT JOIN mrr_DESTCODES d ON i.DESTCODE = d.DESTCODE
LEFT JOIN mrr_STATES s ON d.STATE = s.STATEID

LEFT JOIN mrr_COUNTRIES co ON s.COUNTRY =  co.COUNTRYCODE

order by c.cust</property>
